
    <div id = "nav">
      <% if current_resident %>
        <h1 style = "color: white;"> Welcome Resident </h1>
      <% elsif current_official %>
        <h1 style = "color: white;"> Welcome Official </h1>
      <% end %>
      <% if current_resident %>
        <h1 id = "complaint" >Complaint Form</h1>
        <%= render :partial => 'zipcodes/complaint_form' %>
      <% end %>
    
      <% if resident_signed_in? %>
        <div class = "logout"><%= link_to('Logout', destroy_resident_session_path, :method => :delete) %>  </div>      
      <% end %>

      <% if official_signed_in? %>
        <div class = "logout"><%= link_to('Logout', destroy_official_session_path, :method => :delete) %></div>      
      <% end %>

    <% if current_resident || current_official %>
      <h1 style = "margin-right:300px;"><%= link_to('Your Dashboard', zipcode_path(@user.zipcode.id)) %><h1>
    <% else %>
      <h1 class = "login" style = "float:left;"> Login <h1>
      <%= render :partial => 'users/resident_login' %>
      <h1 class = "sign_up" style = "float:left;"> Signup <h1>
      <%= render :partial => 'users/resident_signup' %>
      <h1 class = "official_login" style = "float:left;"> Official Login <h1>
      <%= render :partial => 'officials/official_login' %>
      <h1 class = "official_signup" style = "float:left; "> Official Signup <h1>
      <%= render :partial => 'officials/official_signup' %>
    <% end %>

	</div>

<div class = "sixteen columns clearfix">


	<div class = "seven columns alpha">
		<div id="map-canvas"></div>
	</div>

	<div class = "nine columns omega" >
		<%= render 'zipcodedash' %>
	</div>	

	


</div>


<div class = "comments">
	<% @zip.comments.each do |comment| %>
				<%= render comment %>
	<% end %>
</div>

<div class="sixteen columns clearfix aggregate" >
	<%= render "zip_aggregate_data" %>

</div>


