

<div id = "main">

	<h1> Most Recent 311 calls </h1>


		<table>
			<% @results.each do |i| %>

				<%= render :partial => 'zipcodes/zipcode', :locals => {:i => i } %>

			<% end %>
		</table>
<div>
		
			<div class = "comments">
				<% @zip.comments.each do |comment| %>

							<%= render comment %>

				<% end %>
			</div>
</div>
	<% if current_resident || current_official and @user.zipcode_id == @zip.id %>

		<%= render :partial => 'comments/form' %>
	<% end %>
		<%= render :partial => 'zipcodes/search' %>




		<div class = "search">

		</div>

</div>